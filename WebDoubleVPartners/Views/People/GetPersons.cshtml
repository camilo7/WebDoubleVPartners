@model List<Person>

<h2>People List</h2>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>UserName</th>
            <th>CreationDate</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.id</td>
                <td>@user.creationDate</td>
                <td>@user.identification</td>
                <td>@user.fullName</td>
            </tr>
        }
    </tbody>
</table>